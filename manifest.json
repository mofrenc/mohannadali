{
    "manifest_version": 3,
    "name": "PixUnPex",
    "version": "1.0",
    "description": "Searches for images on Unsplash, Pexels, and Pixabay.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "permissions": ["tabs"]
  }
  